{
	"source": {
		"include": ["./src/js"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleaverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"template": "./node_modules/jsdoc/templates/jsdoc-template-master",
		"recurse": true,
		"destination": "./docs/"
	}
}
